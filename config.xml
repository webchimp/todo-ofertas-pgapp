        <icon height="1024" platform="ios" src="www/assets/resources/icon/ios/icon-1024.png" width="1024" />
    </platform>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <gap:plugin name="cordova-plugin-facebook4" source="npm" spec="1.7.4">
        <param name="APP_ID" value="170045566912861" />
        <param name="APP_NAME" value="TodoOfertas" />
    </gap:plugin>
    <plugin name="cordova-plugin-battery-status" spec="1.1.2" />
    <plugin name="cordova-plugin-console" spec="1.0.7" />
    <plugin name="cordova-plugin-contacts" spec="2.0.1" />
    <plugin name="cordova-plugin-device" spec="1.1.7" />
    <plugin name="cordova-plugin-device-motion" spec="1.2.5" />
    <plugin name="cordova-plugin-device-orientation" spec="1.0.7" />
    <plugin name="cordova-plugin-dialogs" spec="1.2.1" />
    <plugin name="cordova-plugin-facebook4" spec="1.7.4" />
