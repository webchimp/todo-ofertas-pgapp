<?xml version='1.0' encoding='utf-8'?>
<widget id="com.webchimp.TodoOfertas" version="1.5.4" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>TodoOfertas</name>
    <description>
		Aplicaci√≥n para la empresa Todo Ofertas.
	</description>
    <author email="soporte@chimp.mx" href="http://elchangodelaweb.com">
		WebChimp Team
	</author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="15" />
    <preference name="orientation" value="portrait" />
    <platform name="android">
        <icon density="ldpi" src="www/assets/resources/icon/android/drawable-ldpi-icon.png" />
        <icon density="mdpi" src="www/assets/resources/icon/android/drawable-mdpi-icon.png" />
        <icon density="hdpi" src="www/assets/resources/icon/android/drawable-hdpi-icon.png" />
        <icon density="xhdpi" src="www/assets/resources/icon/android/drawable-xhdpi-icon.png" />
        <icon density="xxhdpi" src="www/assets/resources/icon/android/drawable-xxhdpi-icon.png" />
        <icon density="xxxhdpi" src="www/assets/resources/icon/android/drawable-xxxhdpi-icon.png" />
        <splash density="land-ldpi" src="www/assets/resources/screen/android/drawable-land-ldpi-screen.png" />
        <splash density="land-mdpi" src="www/assets/resources/screen/android/drawable-land-mdpi-screen.png" />
