        <icon height="100" platform="ios" src="www/assets/resources/icon/ios/icon-50@2x.png" width="100" />
        <icon height="60" platform="ios" src="www/assets/resources/icon/ios/icon-60.png" width="60" />
        <icon height="120" platform="ios" src="www/assets/resources/icon/ios/icon-60@2x.png" width="120" />
        <icon height="180" platform="ios" src="www/assets/resources/icon/ios/icon-60@3x.png" width="180" />
